<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE pkgmetadata SYSTEM "http://www.gentoo.org/dtd/metadata.dtd">
<pkgmetadata>
<maintainer>       
<email>maintainer-needed@gentoo.org</email>                       
</maintainer>                               
	<use>
		<flag name='ace'>Enable .ace extraction via
			<pkg>app-arch/unace</pkg></flag>
		<flag name='compress'>Enable tar.Z and *.Z extraction via
			<pkg>app-arch/ncompress</pkg></flag>
		<flag name='cpio'>Enable .cpio extraction via
			<pkg>app-arch/cpio</pkg></flag>
		<flag name='rar'>Enable .rar extraction via
			<pkg>app-arch/unrar</pkg></flag>
		<flag name='rpm'>Enable .rpm extraction via rpm2cpio from
			<pkg>app-arch/rpm</pkg></flag>
		<flag name='uuencode'>Enable .uue extraction via
			<pkg>app-arch/sharutils</pkg></flag>
		<flag name='zip'>Enable .zip extraction via <pkg>app-arch/unzip</pkg>
			and <pkg>app-arch/zip</pkg></flag>
	</use>
</pkgmetadata>
